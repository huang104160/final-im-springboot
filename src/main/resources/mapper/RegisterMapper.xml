<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.im.dao.RegisterDao">
    <insert id="register" parameterType="com.test.im.pojo.User">
        insert into user(`id`,`username`,`password`,`image`)
        values(#{id},#{username},#{password},#{image})
    </insert>
</mapper>