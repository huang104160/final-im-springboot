<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.im.dao.CristicDao">

    <insert id="insertText" parameterType="com.test.im.pojo.Comment">
        insert into comments(`id`,`uid`,`username`,`text`)
        values(#{id},#{uid},#{username},#{text})
    </insert>



</mapper>